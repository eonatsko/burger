<h1>Eat a Burger!</h1>


<ul>
  {{#each burgers}}
      
  {{/each}}
</ul>


<ul>
  {{#each burgers}}
    {{#if devoured}}
    {{/if}}
  {{/each}}
</ul>

<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>

  <button type="submit">Submit</button>
  <p>Enter a burger you'd like to eat and click submit</p>
</form>
